package ch06.unit03;

public class Ex03 {

	public static void main(String[] args) {
		Test3 t = new Test3();
		
		int[] num = {2,4,6,8,10};
	
		int s = t.total(num);
		System.out.println("결과 : " + s);
	}
}

class Test3{
	// 인자로 넘어온 정수형 배열의 합구하기
	public int total(int[] num) {
		int s = 0;
		
		for(int n: num) {
			s += n;
		}
		return s;
	}
	
	
}
