package ch05.unit01.sortEx;

public class Ex03_Bubble {

	public static void main(String[] args) {
		// 개선된 Bubble Sort
		int[] num = new int[] { 25, 15, 7, 4, 12, 9, 17, 6, 20, 13 };
		int t;
		boolean flag = false;

		System.out.print("source data : ");
		for (int n : num) {
			System.out.print(n + " ");
		}
		System.out.println();
	}
}
