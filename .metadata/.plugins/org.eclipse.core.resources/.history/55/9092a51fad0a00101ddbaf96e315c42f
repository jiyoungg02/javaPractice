package ch05.unit02;

public class Quiz05 {

	public static void main(String[] args) {
		int [][]num = new int[4][5];
		int n = 0;
		
		
		for(int i=0; i<9; i++) {
			for(int j=1; j<6; j++) { 
				
			}
		}
		
		for(int i=0; i<num.length; i++) {
			for(int j=0; j<num[i].length; j++) {
				System.out.printf("%3d ", num[i][j]);
			}
			System.out.println();
		}
		
	}
}
