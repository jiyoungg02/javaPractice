package ch05.unit02;

public class Quiz02 {

	public static void main(String[] args) {
		char [][]ch = new char[5][5];
		char a = 'A';
		
		for(int i=0; i<5; i++) {
			for(int j=4; j>=0; j--) {
				ch[j][i] = a;
				a++;
			}
		}
		
		for(int i=0; i<5; i++) {
			for(int j=0; j<5; j++) {
				System.out.printf("%c ", ch[i][j]);
			}
			System.out.println();
		}
	}
}
