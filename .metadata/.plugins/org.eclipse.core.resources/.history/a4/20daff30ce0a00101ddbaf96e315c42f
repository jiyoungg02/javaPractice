package ch06.unit02;

public class Ex01 {

	public static void main(String[] args) {
		
		
	}
}

// 클래스 : 데이터(필드)+ 기능(메소드)으로 구성

class Test1 {
	// 필드선언(인스턴스 변수)
	String name;
	int score;
	int age;
	
	// 메소드(인스턴스 메소드)
	public void write() {
		System.out.println(name + " : " + score + " : " + age);
	}
	
}